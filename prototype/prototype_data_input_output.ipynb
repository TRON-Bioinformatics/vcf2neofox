   "source": [
    "# for a given list of exons, fetch the corresponding sequence from the reference genome, join into the CDS and translate into protein sequence\n",
    "cds = \"\"\n",
    "for _, exon in exons.iterrows():\n",
    "    cds = cds + reference_genome.fetch(exon.seqname.strip(\"chr\"), exon.start, exon.end)\n",
    "        \n",
    "Seq(cds).translate()"
   ]
  },
