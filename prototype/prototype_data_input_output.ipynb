   "source": [
    "# fetch all exons for a given transcript\n",
    "# NOTE: beware to read elements of feature_type=CDS and not feature_type=exon, the later include the UTR regions which we do not want\n",
    "transcript_id = overlapping_transcripts.transcript_id.iloc[0]\n",
    "exons = filtered_gtf[(filtered_gtf.transcript_id == transcript_id) & (filtered_gtf.feature == \"CDS\")].sort_values(\"start\")\n",
    "exons"
   ]
  },
